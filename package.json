	"license": "MIT",
	"scripts": {
		"nx:serve:dashboard": "nx serve dashboard-app --verbose",
		"nx:serve:dashboard:doppler": "doppler run -c loc -p exam-genius -- nx serve dashboard-app",
		"ngrok": "ngrok http --domain=present-shepherd-shining.ngrok-free.app 4200 --config=./bin/ngrok.yml",
		"nx:serve:dashboard:debug": "set NODE_OPTIONS='--inspect' && nx serve dashboard-app",
		"nx:lint:dashboard": "nx lint dashboard-app",
		"nx:lint:landing": "nx lint landing-page",
		"nx:build:dashboard": "nx build dashboard-app --skip-nx-cache",
		"nx:build:dashboard:prod": "npx prisma generate && nx build dashboard-app --prod --skip-nx-cache",
		"nx:serve:dashboard:prod:local": "nx serve dashboard-app --prod",
		"nx:serve:dashboard:prod": "nx serve dashboard-app --prod --hostname 0.0.0.0",
		"nx:test:dashboard:local": "nx build dashboard-app && nx serve dashboard-app --prod",
		"nx:serve:landing": "nx serve landing-page",
		"nx:build:landing": "nx build landing-page",
		"nx:build:landing:prod": "nx build landing-page --prod --skip-nx-cache",
		"nx:serve:landing:prod:local": "nx serve landing-page --prod",
		"nx:test:landing:local": "nx build landing-page && nx serve landing-page --prod",
		"prisma:sync": "npx prismerge -i prismerge.json && npx prisma generate && npx prisma db push",
		"prisma:generate": "npx prismerge -i prismerge.json && npx prisma generate",
		"prisma:studio": "npx prisma studio",
		"prisma:push": "npx prisma db push",
		"prisma:pull": "npx prisma db pull",
		"create-env-file:dashboard": "doppler secrets download --no-file --format env > ./apps/dashboard-app/.env -p exam-genius -c loc",
		"webhookthing": "npx webhookthing@latest",
		"stripe-local-webhook": "stripe listen --forward-to localhost:4200/api/stripe/webhook",
		"dopper:sync-env": "doppler secrets download --no-file --format env > .env.doppler"
	},
	"private": true,
