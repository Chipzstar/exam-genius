model Course {
  id         Int       @id @unique @default(autoincrement())
  course_id  String    @unique
  name       String
  code       String?
  user_id    Int
  user       User      @relation(fields: [user_id], references: [id], onDelete: Cascade)
  exam_board ExamBoard
  product_id String
  year_level Int

  @@index([user_id])
}

enum ExamBoard {
  edexcel
  aqa
  ocr
}
